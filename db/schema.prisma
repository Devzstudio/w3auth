generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model custom_fields {
  option_id String   @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  name      String?
  label     String?
  required  Boolean? @default(false)
}

model user_address {
  user_address_id String  @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  user_ud         String? @db.Uuid
  wallet_address  String?
  chain           String?
}

model user_custom_field {
  field_id  String  @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  option_id String? @db.Uuid
  value     String?
}

model user_logins {
  user_login_id String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  user_id       String?
  browser       String?
  ip            String?
  country       String?
  created_at    DateTime? @db.Timestamptz(6)
}

model users {
  id                String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  name              String?
  nounce            String?
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  email             String?
  kyc_verified      Boolean?  @default(false)
  discord_username  String?
  telegram_username String?
  twitter_username  String?
  last_login        DateTime? @db.Timetz(6)
}

model settings {
  name  String  @id
  value String?
}

model admins {
  id             BigInt    @id @default(autoincrement())
  wallet_address String?
  created_at     DateTime? @default(now()) @db.Timestamptz(6)
  last_login     DateTime? @db.Timestamptz(6)
  nounce         String?
}

model allowlist {
  allowlist_id String    @id @db.Uuid
  address      String?
  label        String?
  created_at   DateTime? @default(now()) @db.Timestamptz(6)
}

model blocklist {
  blocklist_id BigInt    @id @default(autoincrement())
  address      String?
  note         String?
  created_at   DateTime? @default(now()) @db.Timestamptz(6)
}
